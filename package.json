{
  "name": "node-typescript-boilerplate",
  "version": "1.0.0",
  "description": "RBAC / ABAC authorization example app",
  "main": "dist/app/server.js",
  "scripts": {
    "build": "rm -rf ./dist && ./node_modules/.bin/tsc",
    "lint": "./node_modules/.bin/tslint -c tslint.json 'src/**/*.ts'",
    "start-production": "NODE_ENV=production node dist/app/server.js",
    "dev": "NODE_ENV=development tsc-watch --onSuccess \"node ./dist/app/server.js\"",
    "template-migration": "npx tsc && ts-node ./node_modules/typeorm/cli --f ./dist/app/config/rdbms.js migration:create -n",
    "add-migration": "npm run build && node ./node_modules/typeorm/cli --f ./dist/app/config/rdbms.js migration:generate -n",
    "migrate": "npm run build && NODE_ENV=production node ./node_modules/typeorm/cli --f ./dist/app/config/rdbms.js migration:run",
    "rollback": "npm run build && node ./node_modules/typeorm/cli --f ./dist/app/config/rdbms.js migration:revert",
    "test": "NODE_ENV=test nyc --reporter=html --reporter=lcov mocha --timeout 20000 dist/test/  --recursive --exit",
    "test-ts": "tsc && NODE_ENV=test nyc --reporter=html --reporter=text mocha --timeout 10000 dist/test/  --recursive --exit",
    "debug": "node --nolazy --nolazy --inspect-brk=9229 dist/app/server.js"
  },
  "dependencies": {
    "@types/lodash": "^4.14.149",
    "@types/swagger-jsdoc": "^3.0.2",
    "@types/uuid-validate": "0.0.1",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.9.1",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "envalid": "^4.2.2",
    "express": "^4.16.4",
    "helmet": "^3.15.1",
    "http-status-codes": "^1.4.0",
    "image-size": "^0.8.3",
    "ioredis": "^4.6.2",
    "jexl": "^2.1.1",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "otplib": "^11.0.1",
    "pg": "^7.8.0",
    "request-promise": "^4.2.2",
    "sharp": "^0.23.3",
    "swagger-jsdoc": "^3.4.0",
    "swagger-ui-express": "^4.0.2",
    "ts-node": "^8.4.1",
    "typeorm": "^0.2.13",
    "typeorm-naming-strategies": "^1.1.0",
    "ua-parser-js": "^0.7.19",
    "uuid": "^3.3.3",
    "uuid-validate": "0.0.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@fluffy-spoon/substitute": "^1.113.0",
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/bcrypt": "^3.0.0",
    "@types/chai": "^4.2.2",
    "@types/compression": "0.0.36",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.1",
    "@types/google-libphonenumber": "^7.4.17",
    "@types/helmet": "0.0.42",
    "@types/ioredis": "^4.0.6",
    "@types/jexl": "^1.1.2",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/mocha": "^7.0.2",
    "@types/multer": "^1.3.10",
    "@types/multer-s3": "^2.7.7",
    "@types/node": "^10.12.26",
    "@types/request-promise": "^4.1.42",
    "@types/sharp": "^0.23.0",
    "@types/sinon": "^7.5.1",
    "@types/supertest": "^2.0.7",
    "@types/swagger-ui-express": "^3.0.0",
    "@types/ua-parser-js": "^0.7.32",
    "@types/uuid": "^3.4.4",
    "chai": "^4.2.0",
    "mocha": "^7.1.0",
    "nyc": "^14.1.1",
    "sinon": "^7.5.0",
    "supertest": "^3.4.2",
    "tsc-watch": "^1.1.39",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3"
  },
